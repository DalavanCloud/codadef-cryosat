<?xml version="1.0"?>
<cd:Record format="binary" name="SIR_L1B_MEAS_DATA" last-modified="2012-01-30" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Description>SIRAL L1B measurement data</cd:Description>
  <cd:Field name="win_delay">
    <cd:Integer>
      <cd:Description>Window delay (two-way) corrected for instrument delays</cd:Description>
      <cd:Unit>1e-12 s</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000000000.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>64</cd:BitSize>
      <cd:NativeType>int64</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="init_ht">
    <cd:Integer>
      <cd:Description>O initial height word</cd:Description>
      <cd:Unit>48.8 ps</cd:Unit>
      <cd:Conversion numerator="48.800000" denominator="1000000000000.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="hpr_ht_rate">
    <cd:Integer>
      <cd:Description>HPR height rate (unit: 3.05 ps/rc)</cd:Description>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="lai">
    <cd:Integer>
      <cd:Description>LAI</cd:Description>
      <cd:Unit>12.5 ns</cd:Unit>
      <cd:Conversion numerator="12.500000" denominator="1000000000.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="fai">
    <cd:Integer>
      <cd:Description>FAI</cd:Description>
      <cd:Unit>12.5/256 ns</cd:Unit>
      <cd:Conversion numerator="12.500000" denominator="256000000000.000000">
        <cd:Unit>s</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="agc_1">
    <cd:Integer>
      <cd:Description>AGC_1 (corrected)</cd:Description>
      <cd:Unit>1e-2 dB</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>dB</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
      <ct:NamedTest id="RangeAGCOrFlagged"/>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="agc_2">
    <cd:Integer>
      <cd:Description>AGC_2 (corrected)</cd:Description>
      <cd:Unit>1e-2 dB</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
