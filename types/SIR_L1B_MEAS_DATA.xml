      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="int_phase_corr">
    <cd:Integer>
      <cd:Description>Internal phase correction</cd:Description>
      <cd:Unit>1e-6 rad</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>rad</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
