  </cd:Field>
  <cd:Field name="tx_pow">
    <cd:Integer>
      <cd:Description>Transmit power</cd:Description>
      <cd:Unit>1e-6 W</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>W</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
