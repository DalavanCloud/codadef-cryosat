      <cd:NativeType>int32</cd:NativeType>
      <ct:NamedTest id="RangeAGCOrFlagged"/>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="tot_fix_gain_rx1">
    <cd:Integer>
      <cd:Description>Total fixed gain Rx 1</cd:Description>
      <cd:Unit>1e-2 dB</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>dB</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
      <ct:NamedTest id="RangeTotalFixedGain"/>
