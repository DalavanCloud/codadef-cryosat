  <cd:Field name="noise_pow_meas">
    <cd:Integer>
      <cd:Description>Noise power measurement</cd:Description>
      <cd:Unit>1e-2 dB</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
