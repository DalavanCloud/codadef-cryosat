      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="instr_rx_range_corr">
    <cd:Integer>
      <cd:Description>Instrument range correction (Rx only antenna)</cd:Description>
      <cd:Unit>mm</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="instr_sig_0_txrx_corr">
    <cd:Integer>
      <cd:Description>Instrument sigma 0 correction (Tx-Rx antenna)</cd:Description>
      <cd:Unit>1e-2 dB</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>dB</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="instr_sig_0_rx_corr">
