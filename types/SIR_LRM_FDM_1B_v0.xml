<?xml version="1.0"?>
<cd:Record format="binary" name="SIR_LRM_FDM_1B_v0" last-modified="2008-07-01" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10">
  <cd:Description>SIRAL L1B LRM/FDM root type</cd:Description>
  <cd:Field name="mph">
    <cd:NamedType id="MPH"/>
  </cd:Field>
  <cd:Field name="sph">
    <cd:NamedType id="SIR_L1B_SPH"/>
  </cd:Field>
  <cd:Field name="dsd">
    <cd:Array format="ascii">
      <cd:Dimension><![CDATA[int(/mph/num_dsd)]]></cd:Dimension>
      <cd:NamedType id="DSD"/>
    </cd:Array>
  </cd:Field>
  <cd:Field name="siral_l1b_mds">
    <cd:Array>
      <cd:Description>SIRAL L1B measurement data set</cd:Description>
      <cd:Dimension><![CDATA[int(/dsd[0]/num_dsr)]]></cd:Dimension>
      <cd:NamedType id="SIR_L1B_LRM_MDSR_v0"/>
    </cd:Array>
    <cd:BitOffset><![CDATA[8 * int(/dsd[0]/ds_offset)]]></cd:BitOffset>
  </cd:Field>
  <ct:NamedTest id="DSDTypeMDSR" path="dsd[0]/ds_type"/>
  <ct:NamedTest id="MPHRefDocL1" path="mph/ref_doc"/>
  <ct:NamedTest id="SequenceCounterStepTOD" path="siral_l1b_mds[]/time_orb_data[]/src_seq_count"/>
  <ct:NamedTest id="BurstCounterStepTOD" path="siral_l1b_mds[]/time_orb_data[]/burst_count"/>
</cd:Record>
